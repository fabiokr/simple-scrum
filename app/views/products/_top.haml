.span-10
  %h2= "#{@product.name} - #{@product.owner}"
.span-6
  =link_to(t('app.product.backlog'), product_stories_path(@product))
.span-8.last
  %select#change-product{:name => 'product_id'}
    %option{:selected => 'selected'}= t('app.product.change')
    - Product.all.collect do |product|
      %option{:value => product.id}= product.name

